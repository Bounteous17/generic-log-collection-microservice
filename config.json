{
    "flask": {
        "debug": "yes",
        "host": "0.0.0.0",
        "port": 46600,
        "x-api-key": "keyboardcat"
    },
    "logging": {
        "ext": ".log",
        "path": "/var/log",
        "fileNames": {
            "generic": "generic-log-collection-microservice.info.log",
            "error": "generic-log-collection-microservice.error.log"
        },
        "format": {
            "generic": "%(asctime)s-%(process)d-%(levelname)s-%(message)s",
            "error": "%(process)d-%(levelname)s-%(message)s"
        }
    }
}